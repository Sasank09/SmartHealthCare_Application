        <activity android:name=".PatientsList" />
        <activity android:name=".DoctorTreatment" />
        <activity android:name=".PatientCheckup" />
        <activity android:name=".PatientUpdate" />
        <activity android:name=".PatientHome" />
        <activity android:name=".AdminHome" />
        <activity android:name=".DoctorHone" />
        <activity android:name=".DoctorRegister" />
        <activity android:name=".PatientRegister" />
        <activity android:name=".Patient" />
        <activity android:name=".Doctor" />
        <activity android:name=".Admin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>